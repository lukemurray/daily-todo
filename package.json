{
  "name": "dailytodo",
  "version": "0.1.0",
  "description": "Simple Daily Todo app",
  "main": "build/src/main.js",
  "scripts": {
    "build": "tsc",
    "webpack-dev": "npx webpack-dev-server --port 8003 --mode development",
    "webpack": "npx webpack --mode production",
    "app": "APP_URL=http://localhost:8003 npx electron .",
    "start": "npm run build && npx concurrently 'npm run webpack-dev' 'npx wait-on http://localhost:8003 && npm run app'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Luke Murray",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/react": "16.8.2",
    "@types/react-dom": "16.8.0",
    "@types/react-router-dom": "4.3.1",
    "awesome-typescript-loader": "5.2.1",
    "concurrently": "^4.1.0",
    "css-loader": "2.1.0",
    "electron": "4.0.4",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-router-dom": "4.3.1",
    "sass-loader": "7.1.0",
    "source-map-loader": "0.2.4",
    "typescript": "3.3.3",
    "wait-on": "^3.2.0",
    "webpack": "4.29.3",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14"
  }
}
